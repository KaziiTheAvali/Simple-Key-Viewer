{
	"schemaVersion": 1,
	"id": "simple-key-viewer",
	"version": "${version}",
	"name": "Simple Key Viewer",
	"description": "This is a simple key viewer based off of the old lotas key viewer.",
	"authors": [
		"Kazii the avali"
	],
	"contact": {
		"homepage": "https://modrinth.com/mod/simple-input-viewer",
		"sources": "https://github.com/KaziiTheAvali/Simple-Key-Viewer"
	},
	"license": "MIT",
	"icon": "assets/simple-key-viewer/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"kazii.skv.SimpleKeyViewer"
		],
		"client": [
			"kazii.skv.SimpleKeyViewerClient"
		],
		"fabric-datagen": [
			"kazii.skv.SimpleKeyViewerDataGenerator"
		],
        "modmenu": [
            "kazii.skv.config.ModMenuIntegration"
        ]
	},
	"mixins": [
		"simple-key-viewer.mixins.json",
		{
			"config": "simple-key-viewer.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.17.2",
		"minecraft": "~1.21.4",
		"java": ">=21",
		"fabric-api": "*",
        "yet_another_config_lib_v3": ">=3.7.1+1.21.4-fabric"
	},
	"suggests": {
      "modmenu_version": ">=13.0.3"
	}
}